/* CSS Document */
*{padding:0;margin:0;-webkit-text-size-adjust: none; -moz-text-size-adjust: none; -ms-text-size-adjust: none;box-sizing: border-box;-ms-box-sizing: border-box;-moz-box-sizing: border-box;-webkit-box-sizing: border-box;font-family:'Noto Sans KR', 'Malgun Gothic', '맑은 고딕','sans-serif';font-size:12px;letter-spacing:-1px;color:#333;}
h1,h2,h3,h4,h5,h6,em,strong,li,dd,a,address{padding:0;margin:0;}
html{overflow-y:scroll;}
html,body{width:100%;height:100%;}
body,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,table,th,tr,td,thead,tbody,form,fieldset,legend,input,textarea,button{margin:0;padding:0;}
ul,ol,li,dl,dt,dd {list-style:none;}
i,em,address{font-style:normal;}
label,button{cursor:pointer;}
button{border:none;outline:none;background:none;}
hr,legend {display:none;}
img,fieldset,iframe {border:none;}
img{vertical-align:top;}
textarea{font-size:16px;}
caption {display:none;}
select {-webkit-appearance: none;-moz-appearance: none;-ms-appearance: none;-o-appearance: none;appearance: none;}
select::-ms-expand {display: none;}
input,select,button{vertical-align:middle;-webkit-appearance: none;border-radius: 0}
input,textarea {-webkit-appearance: none;-webkit-border-radius: 0;border:1px solid #e0e0e0;}
input[type="text"],input[type="password"] {padding:0 5px;}
input[type="text"]:disabled,input[type="password"]:disabled {background:#f9f9f9;}
textarea {padding:5px;}
table{border:none; border-collapse:collapse; padding:0; border-spacing:0;table-layout: fixed;width:100%;}
u {text-decoration:none;}
i {font-style:normal;}
/* Chrome/Opera/Safari */
::-webkit-input-placeholder {font:16px 'Noto Sans KR','Malgun Gothic', '맑은 고딕','sans-serif';line-height:46px;color:#969696;}
/* Firefox 19+ */
::-moz-placeholder {font:16px 'Noto Sans KR','Malgun Gothic', '맑은 고딕','sans-serif';line-height:46px;color:#969696;}
/* IE 10+ */
:-ms-input-placeholder {font:16px 'Noto Sans KR','Malgun Gothic', '맑은 고딕','sans-serif';line-height:46px;color:#969696;}
/* Firefox 18- */
:-moz-placeholder {font:16px 'Noto Sans KR','Malgun Gothic', '맑은 고딕','sans-serif';line-height:46px;color:#969696;}

/*바로가기*/
.skip a{display:block;position:absolute;left:0;top:-9999em;width:100%;background-color:#272727;color:#aaa;font: bold 14px/3.4 arial,sans-serif;text-align:center;z-index: 99999;}
.skip a:hover,.skip a:active,.skip a:focus{top:0}

/*기본*/
#wrap{width:100%;position:relative;background:#fff;}
#header{position:relative;}
#container{position:relative;*zoom:1;padding-top:111px;}
#container.fixed {padding-top:82px;}
#container:after{display:block;clear:both;content:'';}
.content_inner {max-width:1080px;margin:0 auto;width:100%;position:relative;}
#footer{position:relative;background:url(../images/footer_bg.jpg) repeat-x center;height:246px;margin-top:30px;}

a{text-decoration:none; color:#000;}
a:link,a:visited,a:active,a:focus{}
a:hover{text-decoration:none;}
.blind {position: absolute;overflow: hidden;clip: rect(0 0 0 0);margin: -1px;width: 1px;height: 1px;}
.hide {display:none !important;}

/* float해제 */
.clearfix:after{content:''; display:block; clear:both;}
.fr {float:right;}
.fl {float:left;}

/*정렬*/
.text-center {text-align:center !important;}
.text-right {text-align:right !important;}
.text-left {text-align:left !important;}

/*display-table*/
.table {display:table;width:100%;}
.row {display:table-row;}
.th {display:table-cell;vertical-align:middle;}
.td {display:table-cell;vertical-align:middle;}
.colspan {max-width: 1px;overflow: visible;}

/*select 스타일*/
.styled-select {height:38px;overflow: hidden;}
.styled-select select {background: transparent;border: none;font-size:12px;width:100%;height:100%;padding:0 12px;color:#333;}
.styled-select.slate {background:#fff url(../images/ico_select.jpg) no-repeat right center;height:26px;display:inline-block;vertical-align:middle;border: 1px solid #e0e0e0;}
.styled-select.slate option {font-size:12px;}
.ui-datepicker select.ui-datepicker-month, .ui-datepicker select.ui-datepicker-year {text-align:center;-webkit-appearance:menulist;}

/*radio 스타일*/
.inline {display:inline-block;vertical-align:middle;}
.radio {color:#333;font-size:14px;position: relative;font-weight:500;}
.inline.radio {margin:5px 20px 5px 5px;}
.radio input[type="radio"] {cursor: pointer;position: absolute;width: 100%;height: 100%;z-index: 1;opacity: 0;filter: alpha(opacity=0);-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";}
.radio span {position:relative;padding-left:24px;font-size:14px;color:#333;display:inline-block;line-height:16px;}
.radio span:after {content: '';width:16px;height:16px;background: #fff;border: 1px solid #e1e1e1;position: absolute;left: 0;top:1px;border-radius: 100%;-ms-border-radius: 100%;-moz-border-radius: 100%;-webkit-border-radius: 100%;box-sizing:border-box;}
.radio input[type="radio"]:checked+span:before {content: '';width:8px; height:8px;position: absolute;background:#b355a0;left:4px;top:5px;z-index: 1;border-radius: 100%;-ms-border-radius: 100%;-moz-border-radius: 100%;-webkit-border-radius: 100%;}
.radio-toolbar {font-size:0;}
.radio-toolbar input[type="radio"] {display: none;}
.radio-toolbar label {display:inline-block;background-color:#fff;font-size:12px;cursor: pointer;color:#666;border-right:1px solid #e0e0e0;border-top:1px solid #e0e0e0;border-bottom:1px solid #e0e0e0;width:100px;height:24px;line-height:24px;text-align:center;transition:all .1s;}
.radio-toolbar label.first {border-left:1px solid #e0e0e0;}
.radio-toolbar input[type="radio"]:checked+label {background-color:#ececec;}

/*checkbox 스타일*/
.checkbox.custom {margin: 0;padding: 0;display: inline-block;}
input[type=checkbox].custom {margin-left: 0;padding: 0;}
input[type=checkbox].css-checkbox {position: absolute;overflow: hidden;clip: rect(0 0 0 0);height: 1px;width: 1px;margin: -1px;padding: 0px;border: 0;}
input[type=checkbox].css-checkbox+label.css-label{padding-left:36px;height:25px;display: inline-block;line-height:25px;background-repeat: no-repeat;background-position: 0 0;font-size:16px;vertical-align: middle;cursor: pointer;opacity: 1;color:#333;}
.checkbox.custom.sm {margin-right:5px;}
.checkbox.custom.sm input[type=checkbox].css-checkbox+label.css-label {height:17px;padding-left:22px;line-height:17px;background-size:17px 34px;}
.checkbox.custom.sm input[type=checkbox].css-checkbox:checked+label.css-label {background-position:0 -17px;}
.checkbox.custom.sm .css-label {background-image:url(../images/common/checkbox2.png);}
.checkbox.custom.sm .css-label span {font-size:16px;height:11px;}
input[type=checkbox].css-checkbox:checked+label.css-label{background-position: 0 -25px;}
input[type=checkbox].css-checkbox:disabled+label.css-label {background-position: 0 -50px;}
input[type=checkbox].css-checkbox:disabled+label.css-label span {color:#999;}
.css-label {background-image: url(../images/common/checkbox.png);}
.css-label span {display:table-cell;vertical-align:middle;height:25px;font-size:16px;color:#999;}

/*slick*/
.slick-slider .slick-arrow {text-indent:-9999em;background-size:100% 100%;position:absolute;}

/*pagination*/
.pagination {text-align:center;font-size:0;}
.pagination a {display:inline-block;width:35px;height:35px;line-height:35px;border:1px solid #fff;font-size:14px;text-align:center;color:#333;font-weight:700;background-color:#fff;margin:0 3px;border-radius:50%;}
.pagination a.prev {text-indent:-9999em;background:#fff url(../images/common/paging_prev.png) no-repeat center;margin-right:15px;border:0;}
.pagination a.next {text-indent:-9999em;background:#fff url(../images/common/paging_next.png) no-repeat center;margin-left:15px;border:0;}
.pagination a.active {border:1px solid #74b1ce;}

/*파일첨부*/
.filebox input[type="file"] {position: absolute; width: 1px; height: 1px; padding: 0; margin: -1px; overflow: hidden; clip:rect(0,0,0,0); border: 0; }
.filebox label { display: inline-block;vertical-align:middle;height:38px;line-height:38px;color:#666; font-size: 16px;background-color: #f9f9f9; cursor: pointer; border: 1px solid #e3e3e3; border-bottom-color: #e2e2e2;padding:0 22px;margin-left:12px;} /* named upload */
.filebox .upload-name { display: inline-block;height:38px;line-height:38px;font-size: 16px; font-family: inherit; line-height: normal; vertical-align: top; border: 1px solid #e3e3e3;-webkit-appearance: none; /* 네이티브 외형 감추기 */ -moz-appearance: none; appearance: none;padding:0 5px;}

/*버튼*/
a[class^="btn_"],
button[class^="btn_"] {display:inline-block;text-align:center;vertical-align:middle;transition:all .3s;}

.layer_bg {display:none;z-index:99;position:fixed;left:0;top:0;width:100%;height:100%;background:#000;
  /* IE 8 */
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=60)";
  /* IE 5-7 */
  filter: alpha(opacity=60);
  /* Netscape */
  -moz-opacity: 0.6;
  /* Safari 1.x */
  -khtml-opacity: 0.6;
  /* Good browsers */
  opacity: 0.6;}

